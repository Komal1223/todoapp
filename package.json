    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^12.1.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.47.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-shell-plugin": "^0.5.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.8",
    "eslint": "7.25.0",
    "eslint-plugin-babel": "^5.3.1",
    "file-loader": "^1.1.11",
    "webpack-shell-plugin-next": "^2.2.2"
  }
