{
  "name": "todoapp",
  "version": "0.1.0",
  "descriptions": "Todo App",
  "scripts": {
    "start": "webpack serve",
    "build": "npm run build:i18n && webpack --config webpack.prod.config.js",
    "build:dev": "npm run build:i18n && webpack --config webpack.prod.config.js --env mode = development",
    "build:i18n": "node scripts/i18n-scanner",
    "build:server": "webpack -p --config ./server/webpack.prod.config.js",
    "build:dev:client": "webpack -p --config ./webpack.prod.config.js --env.isDev",
    "lint": "eslint ./src/ -f stylish",
    "lint:fix": "eslint ./src/ -f stylish --fix"
  },
  "dependencies": {
    "@babel/core": "^7.14.8",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-modules-commonjs": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.14.5",
    "@material-ui/core": "^4.12.2",
    "@material-ui/icons": "^4.11.2",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "compression-webpack-plugin": "^8.0.1",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^2.1.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fs": "0.0.1-security",
    "glob": "^7.1.7",
    "glob-watcher": "^5.0.5",
    "html-webpack-include-assets-plugin": "^2.0.0",
    "html-webpack-plugin": "^5.3.2",
    "i18next": "^20.3.5",
    "inject-html-webpack-plugin": "^1.0.1",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^1.6.2",
    "mkdirp": "^1.0.4",
    "node-sass": "^4.14.1",
    "path": "^0.12.7",
    "progress": "^2.0.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
